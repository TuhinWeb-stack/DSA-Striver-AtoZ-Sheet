#include<iostream>
#include<vector>
using namespace std;

void selectionSort(vector<int>&arr){
    int n= arr.size();
    for(int i=0;i<n-1;i++){
        int minIndex= i;
        for(int j=i+1;j<n;j++){
            if(arr[j]<arr[minIndex]){
                minIndex=j;
            }
        }
        swap(arr[minIndex],arr[i]);
    }
}

int main(){
    vector<int>&arr={2,3,8,5,9,3,4,1};
    seelectionSort(arr);

    for(int i=0;i<n;i++){
        cout<<arr<<endl;
    }
    
}
#include<iostream>
#include<vector>
using namespace std;

void largest(vector<int>&arr){
    int n= arr.size();
    int largest = arr[0];
    for(int i=0;i<n;++i){
        if(arr[i]>largest){
            largest=arr[i];
        }
    }
}

int main(){
    int arr[]={} ;
    int n;
    cout<<"Enter the number of elements:"<<endl;
    cin>>n ;
    cout<<"Enter the Elements:"<<endl;
    for(int i=0; i<n;i++){
        cin>>arr>>endl;
    }

    largest(arr);
    cout<<arr<endl;
    return 0;

}
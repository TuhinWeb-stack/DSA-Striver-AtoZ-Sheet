#include<iostream>
using namespace std;{

    void passbyValue(int x){
        x = x+10;
        std :: cout << "Inside function: " << x << std:: endl;
    }

    void passbyReference(int x){
        x = x+10;
        std:: cout<<"Inside function: "<<x<<std::endl;
    }

    int main{}
    int a=5;

    passbyValue(a);

    passbyReference(a);
}
#include<iostream>
using namespace std;

class Node{
    public:
    int data;
    Node*prev;
    Node*next;

    Node(int d){
        this->data;
        this->prev=NULL;
        this->next=NULL;    
    }
};

void print(Node* &head){
    Node*temp=head;
    int length=0;
    while (temp!=NULL)
    {
        cout<<"Data in linked list is:  "<<temp->data<<endl;
        temp=temp->next;
        length++;
    }
    
}

int main(){
    Node* node1= new Node(10);

    Node* head=node1;
    Node* tail=node1;
    print(head);
    return 0;
}